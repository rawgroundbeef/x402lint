{
  "name": "x402check",
  "version": "0.3.1",
  "description": "Validate x402 payment configurations (alias for x402lint)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/"
  ],
  "bin": {
    "x402check": "./dist/cli.mjs"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "test": "echo 'tests run via x402lint'",
    "lint": "tsc --noEmit",
    "prepublishOnly": "pnpm build && publint"
  },
  "keywords": [
    "x402",
    "payment",
    "validation"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rawgroundbeef/x402check.com.git"
  },
  "homepage": "https://www.x402lint.com",
  "bugs": {
    "url": "https://github.com/rawgroundbeef/x402check.com/issues"
  },
  "dependencies": {
    "x402lint": "workspace:*"
  },
  "devDependencies": {
    "publint": "^0.3.16",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3"
  }
}
