{
  "name": "x402lint",
  "version": "0.3.1",
  "description": "Validate x402 payment configurations",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/"
  ],
  "bin": {
    "x402lint": "./dist/cli.mjs"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm test && pnpm build && publint",
    "postpublish": "cd ../x402check && npm version $npm_package_version --no-git-tag-version && pnpm build && npm publish --access public"
  },
  "keywords": [
    "x402",
    "payment",
    "validation",
    "lint"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rawgroundbeef/x402check.com.git"
  },
  "homepage": "https://www.x402lint.com",
  "bugs": {
    "url": "https://github.com/rawgroundbeef/x402check.com/issues"
  },
  "devDependencies": {
    "@noble/hashes": "^2.0.1",
    "@scure/base": "^2.0.0",
    "@types/node": "^25.1.0",
    "c32check": "^2.0.0",
    "cli-table3": "^0.6.5",
    "publint": "^0.3.16",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
